											<Label x:Name="lblBox5" HorizontalAlignment="Left" Height="29" Content="Flow Rate (Q)" VerticalAlignment="Top" Width="190" Margin="0,10,0,0" BorderThickness="0.5,0.5,0,0" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
