									<Grid>
										<WrapPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Left" Height="230">
											<Label x:Name="lblBox1" HorizontalAlignment="Left" Height="29" Content="Density of Cuttting (Ps)" VerticalAlignment="Top" Width="190" Margin="0,0,0,0" BorderThickness="0.5,0.5,0,0" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
											<Label x:Name="lblBox2" HorizontalAlignment="Left" Height="29" Content="Density of Fluids (Pf)" VerticalAlignment="Top" Width="190" Margin="0,10,0,0" BorderThickness="0.5,0.5,0,0" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
