										<Label x:Name="lblBox17" HorizontalAlignment="Left" Height="29" Content="Diameter of Cuttings (Dcut)" VerticalAlignment="Top" Width="190" Margin="0,10,0,0" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
										<Label x:Name="lblBox18" HorizontalAlignment="Left" Height="29" Content="Rate of Penetration (ROP)" VerticalAlignment="Top" Width="190" Margin="0,10,0,0" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
										<Label x:Name="lblBox19" HorizontalAlignment="Left" Height="29" Content="Cutting Concentration(Cconc) " VerticalAlignment="Top" Width="188" Margin="0,10,0,0" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
									</WrapPanel>
								</Grid>
								<Grid>
									<WrapPanel Orientation="Vertical" VerticalAlignment="Center" Width="140" Margin="10,0,-10,0" Height="250">
										<TextBox x:Name="HHtextBoxDh" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="0,3,0,10"/>
										<TextBox x:Name="HHtextBoxDp" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="0,6,0,9"/>
										<TextBox x:Name="HHtextBoxDcut" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="0,7,0,9"/>
										<TextBox x:Name="HHtextBoxRop" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="0,8,0,9"/>
										<TextBox x:Name="HHtextBoxCcon" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="0,7,0,9"/>
									</WrapPanel>
								</Grid>
							</WrapPanel>
						</Grid>
						<Button x:Name="Hbutton" Content="Button" Margin="238,102,-121.8,56.4" Click="HopkinHerschel_Button_Click_Calc" RenderTransformOrigin="0.5,0.5"/>
					</UniformGrid>
				</Grid>
			</TabItem>
		</TabControl>

	</Grid>
</UserControl>
