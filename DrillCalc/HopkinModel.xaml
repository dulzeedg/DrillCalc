<UserControl x:Class="DrillCalc.HopkinModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 TextElement.Foreground="{DynamicResource MaterialDesignBody}"
             TextElement.FontWeight="Regular"
             TextElement.FontSize="13"
             TextOptions.TextFormattingMode="Ideal"
             TextOptions.TextRenderingMode="Auto"
             Background="{DynamicResource MaterialDesignPaper}"
            FontFamily="{DynamicResource MaterialDesignFont}">
	<Grid Height="540" Width="1010">


		<TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="425" Margin="150,55,0,0" VerticalAlignment="Top" Width="625">
			<TabItem Header="Bingham Model">
				<Grid>
					<UniformGrid>
						<Grid>
							<WrapPanel Orientation="Vertical" Margin="0,0,-2,-143.4" >
								<TextBox x:Name="textBox11" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="RPM 600" VerticalAlignment="Top" Width="200" Margin="0,0,0,20" Background="#FF1EB671" />
								<TextBox x:Name="textBox21" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="RPM 300" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="textBox31" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Hole Diameter (Dh)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20" />
								<TextBox x:Name="textBox41" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Pipe Diameter (Dp)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="textBox51" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Mud Weight (pm)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="textBox61" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="D50" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="textBox71" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Density of Cuttings (ps)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="textBox81" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Inclination angle" VerticalAlignment="Top" Width="200" Margin="0,0,0,0"/>
							</WrapPanel>
						</Grid>
						<Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="92.2,296,0,-135.4" VerticalAlignment="Top" Width="120" Height="25" Click="Button_Click"/>
					</UniformGrid>
				</Grid>
			</TabItem>
			<TabItem Header="Herschel-Bulkley Model">
				<Grid>
					<UniformGrid>
						<Grid>
							<WrapPanel Orientation="Vertical"  Margin="0,0,-2,-143.4" >
								<TextBox x:Name="HtextBox11" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="RPM 600" VerticalAlignment="Top" Width="200" Margin="0,0,0,20" Background="#FF1EB671" />
								<TextBox x:Name="HtextBox21" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="RPM 300" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="HtextBox31" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Hole Diameter (Dh)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20" />
								<TextBox x:Name="HtextBox41" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Pipe Diameter (Dp)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="HtextBox51" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Mud Weight (pm)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="HtextBox61" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="D50" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="HtextBox71" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Density of Cuttings (ps)" VerticalAlignment="Top" Width="200" Margin="0,0,0,20"/>
								<TextBox x:Name="HtextBox81" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Inclination angle" VerticalAlignment="Top" Width="200" Margin="0,0,0,0"/>
							</WrapPanel>
						</Grid>
						<Button x:Name="Hbutton" Content="Button" HorizontalAlignment="Left" Margin="92.2,296,0,-135.4" VerticalAlignment="Top" Width="120" Height="25" Click="Button_Click"/>
					</UniformGrid>
				</Grid>
			</TabItem>
		</TabControl>

	</Grid>
</UserControl>
