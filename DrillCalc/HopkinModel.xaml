						<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" MinWidth="362.286"/>
								<ColumnDefinition Width="Auto" MinWidth="230.286"/>
							</Grid.ColumnDefinitions>
							<UniformGrid Grid.ColumnSpan="2" Margin="0,0,10.571,-36.571">
