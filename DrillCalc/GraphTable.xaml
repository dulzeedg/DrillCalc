<Window x:Class="DrillCalc.GraphTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DrillCalc"
        mc:Ignorable="d"
        Title="GraphTable" Height="450" Width="800">
	<Grid>
		<Grid HorizontalAlignment="Left" Height="380" Margin="60,20,0,0" VerticalAlignment="Top" Width="680">
			<DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="300" Margin="44,60,0,0" VerticalAlignment="Top" Width="575" AutoGenerateColumns="True" ItemsSource="{Binding Customer}"  />
			<TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="180,20,0,0" TextWrapping="Wrap" Text="Vmin values for angles 1 - 90" VerticalAlignment="Top" Height="30" Width="270" FontSize="18" FontFamily="Verdana"/>
		</Grid>

	</Grid>
</Window>
